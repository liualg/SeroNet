{
  "$schema":"http://json-schema.org/draft-04/schema",
  "title":"reagents.Array.json template schema",
  "description":"The array reagent template defines and annotates microarrays assay platforms.",
  "type":"object",
  "properties":{
    "userDefinedId":{
    "type":"string","maxLength":100
    },
    "name":{
    "type":"string","maxLength":200
    },
    "description":{
    "type":"string","maxLength":4000
    },
    "manufacturer":{
    "type":"string","maxLength":100
    },
    "catalogNumber":{
    "type":"string","maxLength":250
    },
    "lotNumber":{
    "type":"string","maxLength":250
    },
    "weblink":{
    "type":"string","maxLength":250
    },
    "contact":{
    "type":"string","maxLength":1000
    }
    },
  "required":["userDefinedId","manufacturer","catalogNumber"]
}