{
  "$schema":"http://json-schema.org/draft-04/schema",
  "title":"HLA_Typing.json template schema",
  "description":"The HLA experiment sample template defines and annotates the assay results for a sample by linking sample, experiment, and results together.",
  "type":"object",
  "properties":{
    "expsampleId":{
    "type":"string"
    },
    "ancestralPopulation":{
    "type":"array","items":{"type":"string","enum":["Australia","Europe","None of the Above","North Africa","North America","North-East Asia","Oceania","Other","South America","South-East Asia","South-West Asia","Sub-Saharan Africa"]}
    },
    "hlaAAllele1":{
    "type":"string"
    },
    "hlaAAllele2":{
    "type":"string"
    },
    "hlaBAllele1":{
    "type":"string"
    },
    "hlaBAllele2":{
    "type":"string"
    },
    "hlaCAllele1":{
    "type":"string"
    },
    "hlaCAllele2":{
    "type":"string"
    },
    "hlaDpa1Allele1":{
    "type":"string"
    },
    "hlaDpa1Allele2":{
    "type":"string"
    },
    "hlaDpb1Allele1":{
    "type":"string"
    },
    "hlaDpb1Allele2":{
    "type":"string"
    },
    "hlaDqa1Allele1":{
    "type":"string"
    },
    "hlaDqa1Allele2":{
    "type":"string"
    },
    "hlaDqb1Allele1":{
    "type":"string"
    },
    "hlaDqb1Allele2":{
    "type":"string"
    },
    "hlaDrb1Allele1":{
    "type":"string"
    },
    "hlaDrb1Allele2":{
    "type":"string"
    },
    "hlaDrb3Allele1":{
    "type":"string"
    },
    "hlaDrb3Allele2":{
    "type":"string"
    },
    "hlaDrb4Allele1":{
    "type":"string"
    },
    "hlaDrb4Allele2":{
    "type":"string"
    },
    "hlaDrb5Allele1":{
    "type":"string"
    },
    "hlaDrb5Allele2":{
    "type":"string"
    },
    "comments":{
    "type":"string","maxLength":500
    }
    },
  "required":["expsampleId","ancestralPopulation"]
}