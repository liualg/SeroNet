{
  "$schema":"http://json-schema.org/draft-04/schema",
  "title":" template schema",
  "description":"",
  "type":"object",
  "properties":{
    "userDefinedId":{
    "type":"string","maxLength":150
    },
    "briefTitle":{
    "type":"string","maxLength":250
    },
    "officialTitle":{
    "type":"string","maxLength":500
    },
    "briefDescription":{
    "type":"string","maxLength":4000
    },
    "description":{
    "type":"string"
    },
    "interventionAgent":{
    "type":"string","maxLength":1000
    },
    "endpoints":{
    "type":"string"
    },
    "sponsoringOrganization":{
    "type":"string","maxLength":250
    },
    "ageUnit":{
    "type":"string","enum":["d.p.c.","Days","Hours","Minutes","Months","Not Specified","Seconds","Weeks","Years"]
    },
    "actualStartDate":{
    "type":"string","pattern":"^[0-9]{2}\\(Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep|Oct|Nov|Dec\\)[0-9]{4}$"
    },
    "hypothesis":{
    "type":"string","maxLength":4000
    },
    "objectives":{
    "type":"string"
    },
    "targetEnrollment":{
    "type":"integer","minimum":1
    },
    "minimumAge":{
    "type":"string","maxLength":40
    },
    "maximumAge":{
    "type":"string","maxLength":40
    }
    },
  "required":["userDefinedId","briefTitle","officialTitle","briefDescription","description","interventionAgent","endpoints","sponsoringOrganization","ageUnit"]
}