{
  "$schema":"http://json-schema.org/draft-04/schema",
  "title":"CyTOF_Derived_data.json template schema",
  "description": "The CYTOF derived data template captures and annotates the assay results for a sample by linking sample, experiment, and interpreted results together.",
  "type":"object",
  "properties":{
      "fileName":{
	  "type":"string",
	  "enum":["CyTOF_Derived_data.json"]
      },
     "name":{
	  "type":"string",
	  "enum":["cytof_derived_data"]
      },
     "schemaVersion":{
	  "type":"string",
	  "enum":["3.36"]
      },
     "templateType":{
	  "type":"string",
	  "enum":["multiple"]
      },
     "validationLevel":{
	  "type":"string"
      },
     "data": {
       "type":"array",
       "items":{
         "$ref":"CyTOF_Derived_data.Datum.json#"
       },
       "minItems":1
     }
  },
  "additionalProperties":false,
  "required":["fileName","name","schemaVersion","templateType","data"]
}